{
  "name": "tailwindcss-preset-base",
  "version": "0.0.1",
  "description": "TailwindCSS base preset for Anchour WordPress / Shopify projects.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anchour/tailwindcss-preset-base.git"
  },
  "scripts": {
    "clean": "./node_modules/.bin/rimraf ./dist && mkdir dist",
    "build": "./node_modules/.bin/rollup -c",
    "watch": "./node_modules/.bin/rollup -cw",
    "build-examples": "npm run build && ./node_modules/.bin/postcss styles.css -o examples/default-build/styles.css"
  },
  "author": "Matt Robitaille <matt@anchour.com>",
  "bugs": {
    "url": "https://github.com/anchour/tailwindcss-preset-base/issues"
  },
  "homepage": "https://github.com/anchour/tailwindcss-preset-base#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "autoprefixer": "^10.2.4",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "tailwindcss": "^2.0.3"
  }
}
